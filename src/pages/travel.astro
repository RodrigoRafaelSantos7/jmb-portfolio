---
import Globe from "../components/Globe.astro";
import siteConfig from "../data/site-config";
import TravelLayout from "../layouts/TravelLayout.astro";
---

<TravelLayout description={siteConfig.description} image={siteConfig.image} showHeader={false}>
    <div class="h-screen flex flex-col justify-center items-center">
    <a
        class="text-white absolute bg-neutral-900 hover:bg-neutral-800 top-4 left-4 px-4 py-2 border-1 border-solid border-neutral-600 rounded-lg"
        href="/">
            Back
        </a
      >
    <Globe />
    </div>
</TravelLayout>
